@model List<AegislabsProject.Models.ModelDTO.KRSDto>

<h2>Data KRS</h2>

<a href="/Home/ExportExcel" class="btn btn-success">Export Excel</a>
<a href="/Home/ExportPDF" class="btn btn-danger">Export PDF</a>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Mahasiswa</th>
            <th>Mata Kuliah</th>
            <th>SKS</th>
            <th>Semester</th>
            <th>Tahun Ajaran</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.NamaMahasiswa</td>
                <td>@item.NamaMataKuliah</td>
                <td>@item.SKS</td>
                <td>@item.Semester</td>
                <td>@item.TahunAjaran</td>
            </tr>
        }
    </tbody>
</table>
